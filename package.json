{
  "name": "auto-api-gen",
  "version": "0.0.1",
  "description": "Generate Express APIs automatically from configuration or endpoint files.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "auto-api-gen": "./bin/entry.js"
  },
  "scripts": {
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "build:main": "tsc -p tsconfig.project.json",
    "build": "npm run build:main && npm run build:scripts",
    "dev": "npx auto-api-gen dev",
    "test": "vitest run --coverage",
    "test:mutation": "npx stryker run && rimraf .stryker-tmp",
    "clean": "rimraf temp && rimraf bin && rimraf dist && rimraf reports && rimraf coverage && rimraf .stryker-tmp"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@types/lodash.debounce": "^4.0.9",
    "chokidar": "^4.0.3",
    "express": "^5.1.0",
    "lodash.debounce": "^4.0.8",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@stryker-mutator/core": "^9.2.0",
    "@stryker-mutator/jest-runner": "^9.2.0",
    "@stryker-mutator/vitest-runner": "^9.2.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/express": "^5.0.5",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^4.0.4",
    "concurrently": "^9.2.1",
    "cpx": "^1.5.0",
    "eslint": "^9.38.0",
    "jiti": "^2.6.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.5",
    "ts-node": "^10.9.2",
    "undici-types": "^7.16.0",
    "vitest": "^4.0.4"
  }
}
